
#include <stdio.h>

void main()
{
   int S;
   
   printf("Enter  \n");
   scanf("%d",&S);
   if(S== 1 )
   {
       printf("BlaBla 1 \n");
   }
   else if(S== 2)
   {
       printf("BlaBla 2 \n");
   }
   else if(S== 3)
   {
       printf("BlaBla 3 \n");
   }
   else
   {
       printf("ERROR");
   }
}
