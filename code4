#include <stdio.h>
void main()
{
    int marks;
    char Grade;
    printf("Enter marks");
    scanf("%d",&marks);
    
    if(marks<=100&&marks>=90)
    {
        Grade='A';
    }
    else if(marks<90&&marks>=80)
    {
        Grade='B';
    }
    else if(marks<80&&marks>=60)
    {
        Grade='C';
    }
    else if(marks<60&&marks>=40)
    {
        Grade='D';
    }
    else if(marks<40)
    {
        Grade='F';
    }
    else
    {
        printf("ERROR");
    }
    
    if(Grade=='F')
    {
        printf("Fail");
    }
    else
    {
        printf("%c",Grade);
    }
}
